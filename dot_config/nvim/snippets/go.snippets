snippet tdt "table test function"
  func Test${1:name}(t *testing.T) {
    t.Parallel()

    testCases := map[string]struct {
      name string
    }{
      "Should ${2:test name}": {}
    }

    for name, tc := range testCases {
      tc := tc

      t.Run(name, func(t *testing.T) {
        t.Parallel()

        ${0:${VISUAL}}
      })
    }
  }

snippet iferr "if error not nil"
  if err != nil {
    return fmt.Errorf("${1:ing}: %w")
  }

snippet ifnerr "if error not nil, return nil and formatted error"
  if err != nil {
    return nil, fmt.Errorf("${1:ing}: %w")
  }
